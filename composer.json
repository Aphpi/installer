{
    "name": "aphpi/installer",
    "description": "Aphpi template installer.",
    "keywords": ["aphpi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Sundermeier",
            "email": "daniel@hof-sundermeier.de"
        }
    ],
    "require": {
        "symfony/console": "^5.0",
        "symfony/process": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "bin": [
        "bin/aphpi"
    ],
    "autoload": {
        "psr-4": {
            "Aphpi\\Installer\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aphpi\\Installer\\Console\\Tests\\": "tests/"
        }
    }
}
